{
  "name": "@jsonlang/object",
  "version": "0.0.0",
  "description": "JsonLang Extension for (Key-Value pair) Object/Map Operations",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/ahmed-medhat-tawfiq"
    },
    {
      "type": "opencollective",
      "url": "https://opencollective.com/jsonlang-js"
    }
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "homepage": "https://jsonlang.dev",
  "scripts": {
    "start": "concurrently --handle-input \"wait-on dist/index.js && nodemon\" \"tsc -w\" ",
    "build": "rimraf ./dist/ && tsc",
    "release": "npm run build && npm publish --access public",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsonlangJs/jsonlang.git"
  },
  "keywords": [
    "JsonLang",
    "json-lang",
    "jsonlang-extension",
    "jsonlang-object",
    "rules",
    "conditions",
    "json-code",
    "JsonRules",
    "json-programming",
    "json-language",
    "json-conditions"
  ],
  "author": "https://github.com/ahmed-medhat-tawfiq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JsonlangJs/jsonlang/issues",
    "email": "ahmed.medhat.tawfiq@gmail.com"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "@types/node": "^18.7.23",
    "concurrently": "^7.6.0",
    "jest": "^29.1.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@jsonlang/core": "*"
  }
}
